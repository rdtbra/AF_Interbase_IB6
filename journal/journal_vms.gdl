define database "journal.gdb";

define relation journal
    status		varying [9],
    date_initialized	date,
    id			short,
    sequence		short,
    use_count		short,
    record_number	long;

define relation databases
    filename		varying [128],
    date_enabled	date,
    sequence_enabled	based on sequence,
    use_count,
    status,
    id,
    handle		long;

define relation journal_files
    filename,
    fiel_type char[1],
    status,
    sequence,
    date_opened		date,
    record_number;
